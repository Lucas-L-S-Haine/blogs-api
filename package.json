{
  "name": "blogs-api",
  "version": "1.1.0",
  "private": true,
  "main": "./src/index.js",
  "scripts": {
    "start": "node .",
    "drop": "npx sequelize-cli --env test db:drop $",
    "seed": "npx sequelize-cli --env test db:seed:all $",
    "precreate": "node tasks/createDatabase.js",
    "create": "npx sequelize-cli --env development db:migrate $",
    "postcreate": "npx sequelize-cli --env test db:migrate $",
    "pretest": "NODE_ENV=test node . &> /dev/null &",
    "test": "jest --runInBand",
    "posttest": "killall node",
    "pretest-coverage": "NODE_ENV=test node . &> /dev/null &",
    "test-coverage": "jest -c jest-coverage.js 2> /dev/null",
    "posttest-coverage": "killall node",
    "debug": "nodemon",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json ."
  },
  "nodemonConfig": {
    "ignore": [
      "tests/*",
      "docs/*"
    ],
    "watch": "src/"
  },
  "jest": {
    "testFailureExitCode": 0,
    "verbose": true
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "eslint-config-trybe-backend": "1.0.3",
    "express": "4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "2.1.0",
    "sequelize": "6.3.4"
  },
  "devDependencies": {
    "axios": "^0.27.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.1",
    "frisby": "2.1.2",
    "jest": "26.0.1",
    "minimist": "^1.2.6",
    "moment": "^2.29.4",
    "nodemon": "^2.0.19",
    "sequelize-cli": "6.2.0",
    "supertest": "4.0.2"
  }
}
